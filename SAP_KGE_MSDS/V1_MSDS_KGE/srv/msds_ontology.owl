    rdfs:domain sds:SafetyDataSheet ;
    rdfs:range sds:DisposalConsiderations ;
    rdfs:comment "Links an SDS to its disposal considerations section." .

sds:hasTransportationInformation a rdf:Property ;
    rdfs:label "has transportation information" ;
    rdfs:domain sds:SafetyDataSheet ;
    rdfs:range sds:TransportationInformation ;
    rdfs:comment "Links an SDS to its transportation information section." .

sds:hasRegulatoryInformation a rdf:Property ;
    rdfs:label "has regulatory information" ;
    rdfs:domain sds:SafetyDataSheet ;
    rdfs:range sds:RegulatoryInformation ;
    rdfs:comment "Links an SDS to its regulatory information section." .

sds:hasOtherInformation a rdf:Property ;
    rdfs:label "has other information" ;
