@prefix sds: <http://example.com/sds_ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes (representing SDS sections)
sds:SafetyDataSheet a rdfs:Class ;
    rdfs:label "Safety Data Sheet" ;
    rdfs:comment "Represents a complete Safety Data Sheet." .

sds:Identification a rdfs:Class ;
    rdfs:label "Identification" ;
    rdfs:comment "Information related to product identification and manufacturer details." .

sds:HazardsIdentification a rdfs:Class ;
    rdfs:label "Hazards Identification" ;
    rdfs:comment "Information about the hazards associated with the product." .

sds:CompositionInformationOnIngredients a rdfs:Class ;
    rdfs:label "Composition/Information on Ingredients" ;
    rdfs:comment "Details about the chemical composition of the product." .

sds:FirstAidMeasures a rdfs:Class ;
    rdfs:label "First Aid Measures" ;
    rdfs:comment "Guidelines for immediate medical attention and treatment." .

sds:FireFightingMeasures a rdfs:Class ;
    rdfs:label "Fire Fighting Measures" ;
    rdfs:comment "Information on how to handle fires involving the product." .

sds:AccidentalReleaseMeasures a rdfs:Class ;
    rdfs:label "Accidental Release Measures" ;
    rdfs:comment "Procedures for containing and cleaning up spills and releases." .

sds:HandlingAndStorage a rdfs:Class ;
    rdfs:label "Handling and Storage" ;
    rdfs:comment "Guidelines for safe handling and storage of the product." .

sds:ExposureControlsPersonalProtection a rdfs:Class ;
    rdfs:label "Exposure Controls/Personal Protection" ;
    rdfs:comment "Information on controlling exposure and recommended personal protective equipment." .

sds:PhysicalAndChemicalProperties a rdfs:Class ;
    rdfs:label "Physical and Chemical Properties" ;
    rdfs:comment "Characteristics of the product's physical and chemical nature." .

sds:StabilityAndReactivity a rdfs:Class ;
    rdfs:label "Stability and Reactivity" ;
    rdfs:comment "Information regarding the product's stability and potential hazardous reactions." .

sds:ToxicologicalInformation a rdfs:Class ;
    rdfs:label "Toxicological Information" ;
    rdfs:comment "Details on the potential health effects of the product." .

sds:EcologicalInformation a rdfs:Class ;
    rdfs:label "Ecological Information" ;
    rdfs:comment "Information regarding the environmental impact of the product." .

sds:DisposalConsiderations a rdfs:Class ;
    rdfs:label "Disposal Considerations" ;
    rdfs:comment "Guidelines for proper disposal of the product and its containers." .

sds:TransportationInformation a rdfs:Class ;
    rdfs:label "Transportation Information" ;
    rdfs:comment "Regulations and guidelines for transporting the product." .

sds:RegulatoryInformation a rdfs:Class ;
    rdfs:label "Regulatory Information" ;
    rdfs:comment "Compliance with national and international regulations." .

sds:OtherInformation a rdfs:Class ;
    rdfs:label "Other Information" ;
    rdfs:comment "Miscellaneous information including revision history and ratings." .

# Sub-Classes for structured data within sections
sds:Manufacturer a rdfs:Class ;
    rdfs:label "Manufacturer" ;
    rdfs:comment "Details of the product manufacturer." .

sds:Ingredient a rdfs:Class ;
    rdfs:label "Ingredient" ;
    rdfs:comment "A chemical component of the product." .

sds:ExposureLimit a rdfs:Class ;
    rdfs:label "Exposure Limit" ;
    rdfs:comment "Occupational exposure limits for a chemical." .

sds:Property a rdfs:Class ;
    rdfs:label "Property" ;
    rdfs:comment "A specific physical or chemical property." .

# Properties (for defining relations)

# General SDS Properties
sds:hasIdentification a rdf:Property ;
    rdfs:label "has identification" ;
    rdfs:domain sds:SafetyDataSheet ;
    rdfs:range sds:Identification ;
    rdfs:comment "Links an SDS to its identification section." .

